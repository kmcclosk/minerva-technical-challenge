{
  "name": "palm-nft-technical-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "test": "jest --watch",
    "prettier:base": "prettier 'src/**'",
    "prettier:check": "yarn prettier:base --check",
    "prettier:write": "yarn prettier:base --write",
